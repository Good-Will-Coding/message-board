<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <div class="message-container">
      <div class="messages">
        <% for(let message of messages) { %>
        <ul>
          <li><% message.user %></li>
          <p><% message.text %></p>
          <p><% message.added %></p>
        </ul>
        <% } %>
      </div>
    </div>
  </body>
</html>
